{  
   "name":"icscf",
   "vendor":"fokus",
   "version":"3.0.1",
   "lifecycle_event":[  
      {  
         "event":"CONFIGURE",
         "lifecycle_events":[
		"bind9_resolv_conf.sh",
  		"bind9_relation_joined.sh",
  		"fhoss_relation_joined.sh",
  		"scscf_relation_joined.sh"
         ]
      },
      {  
         "event":"INSTANTIATE",
         "lifecycle_events":[ 
  		"icscf_install.sh"
         ]
      },
      {  
         "event":"START",
         "lifecycle_events":[
  		"icscf_generate_config.sh",
  		"icscf_start.sh" 
         ]
      }
   ],
   "configurations":{  
      "name":"client-configuration",
      "configurationParameters":[
         {  
            "confKey":"port",
            "value":"5060"
         },
         {  
            "confKey":"name",
            "value":"icscf"
         },
         {  
            "confKey":"diameter_port",
            "value":"3869"
         }
      ]
   },
   "vdu":[  
      {  
         "vm_image":["ubuntu:14.04"],
         "scale_in_out":1,
         "vnfc":[  
            {  
               "connection_point":[  
                  {  
  		   "floatingIp":"random",
                   "virtual_link_reference":"mgmt"
                  } 
               ]
            }
         ],
         "vimInstanceName":["docker"]
      }
   ],
   "virtual_link":[  
      {  
         "name":"mgmt"
      }
   ],
   "deployment_flavour":[  
      {  
         "flavour_key":"m1.small"
      }
   ],
   "type":"icscf",
   "endpoint":"docker"
}
