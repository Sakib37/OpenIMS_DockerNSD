{  
   "name":"fhoss",
   "vendor":"fokus",
   "version":"3.0.1",
   "lifecycle_event":[  
      {  
         "event":"CONFIGURE",
         "lifecycle_events":[
		"bind9_resolv_conf.sh",
  		"bind9_relation_joined.sh",
  		"icscf_relation_joined.sh",
  		"scscf_relation_joined.sh"
         ]
      },
      {  
         "event":"INSTANTIATE",
         "lifecycle_events":[ 
  		"fhoss_install.sh"
         ]
      },
      {  
         "event":"START",
         "lifecycle_events":[
  		"fhoss_generate_config.sh",
  		"fhoss_start.sh"
         ]
      }
   ],
   "configurations":{  
      "name":"client-configuration",
      "configurationParameters":[
         {  
            "confKey":"diameter_port",
            "value":"3868"
         },
         {  
            "confKey":"name",
            "value":"hss"
         },
         {
            "confKey":"web_port",
            "value":"8000"
         }

      ]
   },
   "vdu":[  
      {  
         "vm_image":["ubuntu:14.04"],
         "scale_in_out":1,
         "vnfc":[  
            {  
               "connection_point":[  
                  {  
  		   "floatingIp":"random",
                     "virtual_link_reference":"mgmt"
                  } 
               ]
            }
         ],
         "vimInstanceName":["docker"]
      }
   ],
   "virtual_link":[  
      {  
         "name":"mgmt"
      }
   ],
   "deployment_flavour":[  
      {  
         "flavour_key":"m1.small"
      }
   ],
   "type":"fhoss",
   "endpoint":"docker"
}
